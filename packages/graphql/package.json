    "@types/lodash.merge": "^4.6.6",
    "@types/uuid": "^8.3.0",
    "apollo-server": "^2.22.2",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "type-fest": "^1.2.2",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@mattb.tech/billio-data": "1.0.0",
    "apollo-server-lambda": "^2.23.0",
    "axios": "^0.21.1",
    "dotenv": "^9.0.0",
    "graphql": "^15.5.0",
    "lodash.merge": "^4.6.2",
    "uuid": "^8.3.2"
  }
