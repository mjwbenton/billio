{
  "name": "@mattb.tech/billio-graphql",
  "private": true,
  "version": "1.0.0",
  "description": "Graphql layer for billio",
  "main": "dist/index.js",
  "author": "Matt Benton <m@mattb.tech>",
  "license": "MIT",
  "scripts": {
    "build": "yarn codegen && ../../scripts/build.sh",
    "clean": "../../scripts/clean.sh",
    "fix": "../../scripts/fix.sh",
    "check": "../../scripts/check.sh",
    "watch": "yarn codegen --watch & ../../scripts/watch.sh",
    "start": "yarn watch & nodemon ./dist/devServer.js",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "^1.21.8",
    "@graphql-codegen/typescript": "^1.23.0",
    "@graphql-codegen/typescript-resolvers": "^1.20.0",
    "@types/aws-lambda": "^8.10.81",
    "@types/lodash.merge": "^4.6.6",
    "@types/uuid": "^8.3.1",
    "apollo-server": "^3.1.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "type-fest": "^1.3.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@graphql-tools/schema": "^7.0.0",
    "@graphql-tools/wrap": "^7.0.0",
    "@mattb.tech/billio-data": "1.0.0",
    "apollo-server-lambda": "^3.1.1",
    "axios": "^0.21.1",
    "dotenv": "^10.0.0",
    "graphql": "^15.5.1",
    "graphql-scalars": "^1.10.0",
    "lodash.merge": "^4.6.2",
    "uuid": "^8.3.2"
  }
}
