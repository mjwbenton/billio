{
  "name": "@mattb.tech/billio-graphql",
  "private": true,
  "version": "1.0.0",
  "description": "Graphql layer for billio",
  "main": "dist/index.js",
  "author": "Matt Benton <m@mattb.tech>",
  "license": "MIT",
  "scripts": {
    "build": "yarn codegen && ../../scripts/build.sh",
    "clean": "../../scripts/clean.sh",
    "fix": "../../scripts/fix.sh",
