    "@mattb.tech/billio-cdk#bootstrap": {
      "dependsOn": [
        "@mattb.tech/billio-admin#build",
        "@mattb.tech/billio-graphql#build",
        "@mattb.tech/billio-backup#build"
      ]
    }
  }
