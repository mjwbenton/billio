{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "check": {
      "outputs": []
    },
    "fix": {
      "outputs": []
    },
    "clean": {},
    "deploy": {
      "outputs": []
    },
    "bootstrap": {
      "outputs": []
    },
    "@mattb.tech/billio-cdk#deploy": {
      "dependsOn": ["^build", "^check"]
    },
    "@mattb.tech/billio-cdk#bootstrap": {
      "dependsOn": ["^build", "^check"]
    }
  }
}
